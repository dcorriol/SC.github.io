                    url: response,
                    type: "get",
                    async: false
                });
            }
            try {
                // support responseText for backward compatibility (pre 5.3)
                return eval("(" + (response.body ? response.body
                    : response.responseText) + ")");
            } catch (e) {
            }
            return null;
        };

        return self;
    }());

